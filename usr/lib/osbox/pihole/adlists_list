
piholeadlists(){
    $OSBOX_ID="$(</etc/osbox/osbox.id)"
    echo "https://blocklists.surfwijzer.nl/category/porn/$OSBOX_ID">/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/proxy/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/advertising/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/malware/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/suspicious/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/group1/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/group2/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/group3/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/group4/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/category/group5/$OSBOX_ID">>/etc/pihole/adlists.list
}

