
piholeadlists(){
    $OSBOX_ID="$(</etc/osbox/osbox.id)"
    echo "https://blocklists.surfwijzer.nl/c/porn/$OSBOX_ID">/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/c/proxy/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/c/ads/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/c/tracking/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/c/shorturl/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/g/1/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/g/2/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/g/3/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/g/4/$OSBOX_ID">>/etc/pihole/adlists.list
    echo "https://blocklists.surfwijzer.nl/g/5/$OSBOX_ID">>/etc/pihole/adlists.list
}

